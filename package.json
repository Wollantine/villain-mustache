    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "mocha": "^3.0.1",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0"
  },
